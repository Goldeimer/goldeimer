{
    "name": "goldeimer/wordpress-theme",
    "description": "Goldeimer's WordPress theme",
    "autoload": {
        "psr-4": {
            "Goldeimer\\WordPressTheme\\": "src/php"
        }
    },
    "config": {
        "vendor-dir": "dist/vendor",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-release": "0.1.3-dev"
        }
    }
}
