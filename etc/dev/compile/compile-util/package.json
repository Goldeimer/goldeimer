{
    "author": "Goldeimer gemeinn√ºtzige GmbH <post@goldeimer.de>",
    "bugs": {
        "email": "engineering+bugs@goldeimer.de",
        "url": "https://github.com/goldeimer/goldeimer/issues"
    },
    "contributors": [
        "Johannes Pilkahn <johannes@goldeimer.de>"
    ],
    "description": "Goldeimer's utility library for shared logic between build systems",
    "devDependencies": {
        "@babel/core": "^7.12",
        "@goldeimer/rollup-config": "workspace:*",
        "@goldeimer/tsconfig": "workspace:*",
        "@types/babel__core": "^7",
        "rollup": "^2.30",
        "tslib": "*",
        "typescript": "~4.1"
    },
    "exports": {
        ".": {
            "import": "./dist/compile-util.esm.mjs",
            "require": "./dist/compile-util.cjs.cjs"
        }
    },
    "homepage": "https://github.com/goldeimer/goldeimer/etc/build/compile-util#readme",
    "keywords": [
        "babel",
        "build",
        "comile",
        "config",
        "goldeimer",
        "goldeimer/build",
        "goldeimer/compile",
        "goldeimer/config",
        "ts",
        "typescript"
    ],
    "license": "BSD-2-Clause",
    "main": "./src/compile-util.js",
    "module": "./src/compile-util.js",
    "name": "@goldeimer/compile-util",
    "repository": {
        "directory": "etc/build/compile-util",
        "type": "git",
        "url": "git+https://github.com/goldeimer/goldeimer.git"
    },
    "scripts": {
        "build": "yarn build:types && yarn build:js",
        "build:cjs": "yarn build:js --configTarget cjs",
        "build:esm": "yarn build:js --configTarget esm",
        "build:js": "rollup -c rollup.config.js",
        "build:shake": "yarn build:esm && yarn shake",
        "build:types": "tsc --emitDeclarationOnly --outDir dist --declarationDir dist/types --declaration true --declarationMap true",
        "build:umd": "yarn build:js --configTarget umd",
        "lint": "eslint src --ext .js --ext .jsx",
        "lint:fix": "eslint src --fix --ext .js --ext .jsx",
        "shake": "agadoo",
        "test": "env NODE_ENV=test echo \"Error: no test specified\" && exit 1",
        "type-check": "tsc --noEmit",
        "type-check:watch": "yarn type-check -- --watch",
        "validate:publish": "exit 0 || yarn shake"
    },
    "type": "module",
    "version": "0.0.1"
}
