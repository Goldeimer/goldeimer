{
    "default": true,
    "first-heading-h1": [true, {
        "level": 1
    }],
    "first-header-h1": [true, {
        "level": 1
    }],
    "heading-increment": true,
    "heading-style":  [true, {
        "style": "setext_with_atx"
    }],
    "header-style":  [true, {
        "style": "setext_with_atx"
    }],
    "list-indent": true,
    "ul-start-left": true,
    "ul-style": [true, {
        "style": "consistent"
    }],
    "ul-indent": [true, {
        "indent": 4
    }],
    "no-trailing-spaces": [true, {
        "br_spaces": 2,
        "list_item_empty_lines": false,
        "strict": true
    }],
    "no-hard-tabs": [true, {
        "code_blocks": true
    }],
    "no-reversed-links": true,
    "no-multiple-blanks": [true, {
        "maximum": 1
    }],
    "line-length": [true, {
        "line_length": 80,
        "heading_line_length": 80,
        "code_block_line_length": 80,
        "code_blocks": true,
        "tables": true,
        "headings": true,
        "headers": true,
        "strict": true,
        "stern": false
    }],
    "commands-show-output": true,
    "no-missing-space-atx": true,
    "no-multiple-space-atx": true,
    "no-missing-space-closed-atx": true,
    "no-multiple-space-closed-atx": true,
    "blanks-around-headings": [true, {
        "lines_above": 1,
        "lines_below": 1
    }],
    "blanks-around-headers": [true, {
        "lines_above": 1,
        "lines_below": 1
    }],
    "heading-start-left": true,
    "header-start-left": true,
    "no-duplicate-heading": [true, {
        "allow_different_nesting": false,
        "siblings_only": false
    }],
    "no-duplicate-header": [true, {
        "allow_different_nesting": false,
        "siblings_only": false
    }],
    "single-title": [true, {
        "level": 1,
        "front_matter_title": "<%= args.titleRegEx %>"
    }],
    "single-h1": [true, {
        "level": 1,
        "front_matter_title": "<%= args.titleRegEx %>"
    }],
    "no-trailing-punctuation": false,
    "no-multiple-space-blockquote": true,
    "no-blanks-blockquote": true,
    "ol-prefix": "one",
    "list-marker-space": [true, {
        "ul_single": 1,
        "ol_single": 1,
        "ul_multi": 1,
        "ol_multi": 1
    }],
    "blanks-around-fences": [true, {
          "list_items": true
    }],
    "no-inline-html": false,
    "allowed_elements": true,
    "no-bare-urls": true,
    "hr-style": [true, { "style": "---" }],
    "no-emphasis-as-heading": [true, {
        "punctuation": "<%= args.punctuation %>"
    }],
    "no-emphasis-as-header": [true, {
        "punctuation": "<%= args.punctuation %>"
    }],
    "no-space-in-emphasis": false,
    "no-space-in-code": true,
    "no-space-in-links": true,
    "fenced-code-language": true,
    "first-line-heading": false,
    "first-line-h1": [true, {
        "level": 1,
        "regEx": "<%= args.titleRegEx %>"
    }],
    "no-empty-links": true,
    "required-headings": [true, {
        "body": null,
        "headers": null
    }],
    "required-headers": [true, {
        "body": null,
        "headers": null
    }],
    "proper-names": [true, {
        "names": [],
        "code_blocks": true
    }],
    "no-alt-text": true,
    "code-block-style": [true, {
        "style": "fenced"
    }],
    "single-trailing-newline": true,
    "code-fence-style": "backtick",
    "headings": true,
    "headers": {
        "bullet": true
    },
    "ul": true,
    "indentation": true,
    "whitespace": true,
    "hard_tab": true,
    "links": true,
    "blank_lines": true,
    "line_length": true,
    "code": true,
    "atx": true,
    "spaces": true,
    "atx_closed": true,
    "blockquote": true,
    "ol": true,
    "html": true,
    "url": true,
    "hr": true,
    "emphasis": true,
    "language": true,
    "spelling": true,
    "accessibility": true,
    "images": true
}
