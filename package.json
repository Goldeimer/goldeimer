{
    "name": "goldeimer-web-apps",
    "version": "0.1.3",
    "private": true,
    "workspaces": {
        "packages": ["packages/*"]
    },
    "scripts": {
        "build:keinebaeume": "lerna exec --scope @goldeimer/abenteuer-regenwald-campaign -- yarn build",
        "build:keinebaeume:webpack": "lerna exec --scope @goldeimer/abenteuer-regenwald-campaign -- yarn build:webpack",
        "build:lib:dev": "lerna exec --scope @goldeimer/map-core -- yarn build:dev",
        "build:lib:prod": "lerna exec --scope @goldeimer/map-core -- yarn build:prod",
        "build:map-core:dev": "lerna exec --scope @goldeimer/map-core -- yarn build:dev",
        "build:map-core:prod": "lerna exec --scope @goldeimer/map-core -- yarn build:prod",
        "check-env": "node -e 'console.log(process.env)' | grep npm"
    },
    "devDependencies": {
        "@babel/core": "^7.10.2",
        "@babel/plugin-proposal-class-properties": "^7.10.4",
        "@babel/plugin-transform-react-jsx-source": "^7.10.1",
        "@babel/plugin-transform-runtime": "^7.10.1",
        "@babel/plugin-transform-unicode-regex": "^7.10.1",
        "@babel/preset-env": "^7.10.2",
        "@babel/preset-react": "^7.10.1",
        "@babel/runtime": "^7.10.2",
        "@goldeimer-config/browserslist-config": "^0.0.1",
        "@goldeimer-config/eslint-plugin": "^0.0.1",
        "@goldeimer-config/webpack": "^0.0.1",
        "@google/clasp": "^2.3.0",
        "@types/react": "^16.9.46",
        "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
        "gas-webpack-plugin": "^1.0.4",
        "html-webpack-inline-source-plugin": "^1.0.0-beta.2",
        "lerna": "^3.22.1",
        "path": "^0.12.7",
        "typescript": "^3.9.7",
        "webpack-merge": "^4.2.2"
    }
}
