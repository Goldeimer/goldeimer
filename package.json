{
    "author": "Goldeimer gemeinnützige GmbH <post@goldeimer.de>",
    "description": "Goldeimer gemeinnützige GmbH's web applications monorepo - root workspace",
    "devDependencies": {
        "@babel/core": "^7.12",
        "@babel/runtime": "^7.12",
        "@goldeimer/dev": "workspace:*",
        "@goldeimer/package-cli": "workspace:etc/dev/package/package-cli",
        "@goldeimer/tsconfig": "workspace:*",
        "@google/clasp": "^2.3.0",
        "@octokit/core": "^3.2",
        "@types/node": "^14.6",
        "@types/react": "^17.0",
        "agadoo": "^2.0.0",
        "concurrently": "^6.0",
        "eslint": "^7.12",
        "eslint-import-resolver-lerna": "^2.0",
        "external-editor": "^3.1",
        "lerna": "^4.0.0",
        "path": "^0.12.7",
        "react": "^17.0",
        "react-dom": "^17.0",
        "rimraf": "^3.0",
        "ts-node": "^9.1.1",
        "typescript": "~4.1"
    },
    "name": "@goldeimer/worktree-root",
    "private": true,
    "resolutions": {
        "@bundle-stats/utils": "^2.6.0",
        "axios": "^0.21.0",
        "follow-redirects": "^1.13.1",
        "fork-ts-checker-webpack-plugin": "^6.0.0",
        "inquirer-autocomplete-prompt": "^1.0.1",
        "rollup": "^2.33.0",
        "rollup-plugin-inject": "npm:@rollup/plugin-inject@^4.0.2",
        "rollup-plugin-virtual": "npm:@rollup/plugin-virtual@^2.0.3",
        "typescript": "~4.1"
    },
    "scripts": {
        "build": "lerna run build",
        "build:cjs": "lerna run build:cjs",
        "build:clean": "yarn clean:install && yarn build",
        "build:dev": "lerna run build:dev",
        "build:esm": "lerna run build:esm",
        "build:js": "lerna run build:js",
        "build:prod": "lerna run build:prod",
        "build:shake": "lerna run build:shake",
        "build:ts": "lerna run build:ts",
        "build:umd": "lerna run build:umd",
        "clean": "concurrently 'yarn clean:cache' 'yarn clean:lockfile' 'yarn clean:pnp'",
        "clean:cache": "yarn cache clean",
        "clean:install": "yarn clean && yarn install",
        "clean:lockfile": "rimraf package-lock.json yarn.lock",
        "clean:mirror": "yarn cache clean --mirror",
        "clean:pnp": "rimraf .pnp?(.data)js?(on)",
        "exec:parallel": "lerna exec --parallel",
        "journal:workspaces": "yarn workspaces:json > etc/journal/workspaces.list.json",
        "lint": "eslint --cache --cache-location etc/journal/cache/eslint",
        "lint:all": "yarn lint 'packages/*/src/**/*.{js,jsx,ts,tsx}*'",
        "lint:all:fix": "yarn lint:all --fix",
        "lint:fix": "yarn lint --fix",
        "lint:staged": "lint-staged",
        "pnp:sdk:vim": "yarn dlx @yarnpkg/pnpify --sdk vim",
        "pnp:sdk:vscode": "yarn dlx @yarnpkg/pnpify --sdk vscode",
        "publish": "lerna publish",
        "publish:etc": "lerna publish --ignore-changes 'lib/**', 'packages/**'",
        "publish:lib": "lerna publish --ignore-changes 'etc/**', 'packages/**'",
        "publish:packages": "lerna publish --ignore-changes 'etc/**', 'lib/**'",
        "run:parallel": "lerna run --parallel",
        "shake": "goldeimer-package",
        "start": "lerna run start",
        "test": "lerna run test --concurrency 1",
        "test:coverage": "lerna run test:coverage --parallel",
        "test:quick": "lerna run test:quick --concurrency 1",
        "test:watch": "lerna run test:watch --parallel",
        "util:process-env": "node -e 'console.log(process.env)' | grep npm",
        "validate:config": "yarn validate:config:yarn",
        "validate:config:yarn": "yarn config -v",
        "validate:pnp": "yarn dlx @yarnpkg/doctor",
        "version": "lerna version",
        "version:etc": "lerna version --ignore-changes 'lib/**', 'packages/**'",
        "version:lib": "lerna version --ignore-changes 'etc/**', 'packages/**'",
        "version:packages": "lerna version --ignore-changes 'etc/**', 'lib/**'",
        "workspaces:json": "yarn workspaces list --json",
        "workspaces:list": "yarn workspaces list",
        "~deprecated:clean:packages": "lerna clean --yes",
        "~deprecated:clean:nm": "rimraf node_modules"
    },
    "workspaces": [
        "etc",
        "lib",
        "packages"
    ]
}
