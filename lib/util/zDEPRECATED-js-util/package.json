    ],
    "dependencies": {
        "@goldeimer/data-util": "workspace:*",
        "@material-ui/core": "^4.11.0",
        "axios": "^0.21.0",
        "change-case": "^4.1.2",
        "is-lower-case": "^2.0.2",
        "is-upper-case": "^2.0.2",
        "lower-case": "^2.0.2",
        "lower-case-first": "^2.0.2",
        "nanoid-generate": "^1.0.0",
        "papaparse": "^5.3.0",
        "sponge-case": "^1.0.1",
        "swap-case": "^2.0.2",
        "title-case": "^3.0.3",
        "upper-case": "^2.0.2",
        "upper-case-first": "^2.0.2"
    },
    "description": "Goldeimer's JavaScript utility library",
    "devDependencies": {
        "@goldeimer/rollup-config": "workspace:*",
        "typescript": "^4.1.5"
    },
    "files": [
        "dist/umd"
    ],
    "homepage": "https://github.com/goldeimer/goldeimer/etc/build/js-util#readme",
    "keywords": [
        "goldeimer",
        "utility"
    ],
    "license": "BSD-2-Clause",
    "main": "dist/goldeimer-js-util.cjs.js",
    "module": "dist/goldeimer-js-util.esm.js",
    "name": "@goldeimer/js-util",
    "peerDependencies": {
        "react": "^17.0.0"
    },
    "repository": {
        "directory": "lib/js-util",
        "type": "git",
        "url": "git+https://github.com/goldeimer/goldeimer.git"
    },
    "scripts": {
        "build": "yarn build:types && yarn build:js",
        "build:cjs": "yarn build --configTarget cjs",
        "build:esm": "yarn build --configTarget esm",
        "build:js": "rollup -c rollup.config.js",
        "build:umd": "yarn build --configTarget umd",
        "build:shake": "yarn build:esm && yarn shake",
        "build:types": "tsc --emitDeclarationOnly --outDir dist --declarationDir dist/types --declaration true --declarationMap true",
        "lint": "eslint src --ext .js --ext .jsx",
        "lint:fix": "eslint src --fix --ext .js --ext .jsx",
        "publish": "yarn validate:publish && yarn npm publish",
        "shake": "agadoo",
        "test": "env NODE_ENV=test echo \"Error: no test specified\" && exit 1",
        "validate:publish": "yarn shake",
        "validate:types": "tsc --noEmit",
        "validate:types:watch": "yarn validate:types -- --watch"
    },
    "sideEffects": false,
    "types": "dist/types/js-util.d.ts",
    "version": "0.0.1"
}
