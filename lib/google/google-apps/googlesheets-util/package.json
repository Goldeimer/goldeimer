    "name": "@goldeimer/google-lib-sheets",
    "repository": {
        "directory": "packages/google-lib-sheets",
        "type": "git",
        "url": "git+https://github.com/goldeimer/goldeimer.git"
    },
    "scripts": {
        "build:dev": "env BUILD_MODE=development webpack --config webpack.config.js",
        "build:prod": "env BUILD_MODE=production webpack --config webpack.config.js",
        "clasp:init": "rm -f .clasp.json && clasp create --type sheets --title $publisherRef --rootDir ./dist/gsheets",
        "clasp:login": "clasp login",
        "clasp:logs": "clasp logs",
        "clasp:set-script-id": "clasp setting scriptId",
        "deploy": "npm run build:prod && clasp push",
        "lint": "eslint src --ext .js --ext .jsx",
        "lint:fix": "eslint src --fix --ext .js --ext .jsx",
        "test": "env NODE_ENV=test echo \"Error: no test specified\" && exit 1"
    },
    "version": "0.0.1"
}
